<?xml version="1.0" encoding="UTF-8"?>
<campaign>
  <campaignName>CHIPTS</campaignName>
  <campaignVersion>0.1</campaignVersion>
  <serverUrl>https://www.andwellness.org</serverUrl>

  <surveys>
    <survey>
      <id>generalFeelingToday</id>
      <title>General Feeling Today</title>
      <description>How are you feeling today?</description>
      <submitText>Thank you for completing this survey.</submitText>
      <showSummary>false</showSummary>
      <anytime>true</anytime>
    
      <contentList>
      
        <prompt>
          <id>generalFeelingTodaySad</id>
          <displayType>measurement</displayType>
          <displayLabel>Sad, blue, or depressed</displayLabel>
          <promptText>Sad, blue, or depressed?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>A little</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Somewhat</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Extremely</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>generalFeelingTodayWorried</id>
          <displayType>measurement</displayType>
          <displayLabel>Worried, tense, or anxious</displayLabel>
          <promptText>Worried, tense, or anxious?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>A little</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Somewhat</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Extremely</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>generalFeelingTodayNotEnoughRest</id>
          <displayType>measurement</displayType>
          <displayLabel>Not get enough rest or sleep</displayLabel>
          <promptText>Not get enough rest or sleep?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>A little</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Somewhat</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Extremely</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        <prompt>
          <id>generalFeelingTodayVeryHealthy</id>
          <displayType>measurement</displayType>
          <displayLabel>Very healthy and full of energy</displayLabel>
          <promptText>Very healthy and full of energy?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>A little</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Somewhat</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Extremely</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
        
        <prompt>
          <id>generalFeelingTodayUsualActivities</id>
          <displayType>measurement</displayType>
          <displayLabel>Usual activities were hard to do</displayLabel>
          <promptText>Usual activities were hard to do?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>A little</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Somewhat</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Extremely</label>
              <value>3</value>
            </property>
          </properties>
          <skippable>false</skippable>
        </prompt>
        
      </contentList>
    
    </survey>
  </surveys>
</campaign>
