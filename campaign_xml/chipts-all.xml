  <surveys>
  
    <survey>
      <id>specificStressfulEvent</id>
      <title>Specific Stressful Event</title>
      <description>Please describe a specific stressful event that took place today.</description>
      <submitText>Thank you for completing this survey.</submitText>
      <showSummary>false</showSummary>
      <anytime>true</anytime>
     
      <contentList>
        
        <prompt>
          <id>specificStressfulEventWhatHappened</id>
          <displayType>category</displayType>
          <displayLabel>What Happened</displayLabel>
          <promptText>What happened?</promptText>
          <promptType>text</promptType>
          <properties>
            <property>
              <key>min</key>
              <label>1</label>
            </property>
            <property>
              <key>max</key>
              <label>500</label> 
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>Skip</skipLabel>
        </prompt>

        <prompt>
          <id>specificStressfulEventHowStressful</id>
          <displayType>category</displayType>
          <displayLabel>How Stressful</displayLabel>
          <promptText>How stressful would you rate this event on a 1-10 scale?</promptText>
          <promptType>number</promptType>
            <properties>
            <property>
              <key>min</key>
              <label>0</label>
            </property>
            <property>
              <key>max</key>
              <label>10</label>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>Skip</skipLabel>
        </prompt>

        <prompt>
          <id>specificStressfulEventTime</id>
          <displayType>category</displayType>
          <displayLabel>What Time</displayLabel>
          <promptText>What time did this happen?</promptText>
          <promptType>timestamp</promptType>
          <skippable>true</skippable>
          <skipLabel>Skip</skipLabel>
        </prompt>

        <prompt>
          <id>specificStressfulEventPhoto</id>
          <displayType>metadata</displayType>
          <displayLabel>Photo</displayLabel>
          <promptText>Take a picture related to the stress or event.</promptText>
          <promptType>photo</promptType>
          <properties>
            <property>
              <key>res</key>
              <label>720</label>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>Skip</skipLabel>
        </prompt>
        
      </contentList>
    
    </survey>
    
