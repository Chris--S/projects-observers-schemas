            <property>
              <key>0</key>
              <label>Male</label>
            </property>
            <property>
              <key>1</key>
              <label>Transgender</label>
            </property>
            <property>
              <key>2</key>
              <label>Female</label>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>Skip</skipLabel>
        </prompt>
        
        <prompt>
          <condition>sexualEncounterHowManyEncountersSinceLastSurvey > 0</condition>
          <id>sexualEncounterPartnerHivStatus</id>
          <displayType>category</displayType>
          <displayLabel>Partner HIV Status</displayLabel>
          <promptText>What is this partner's HIV status?</promptText>
          <promptType>single_choice</promptType>
          <properties>
            <property>
              <key>0</key>
              <label>Positive</label>
            </property>
            <property>
              <key>1</key>
