  <campaignUrn>urn:andwellness:moms</campaignUrn>
  <campaignName>Moms</campaignName>
  <serverUrl>https://heart.andwellness.org</serverUrl>

  <surveys>
    <survey>
      <id>Morning</id>
      <title>Morning</title>
      <description>This is the morning survey.</description>
      <submitText>Please use the food button on your phone to to take pictures of your food.</submitText>
      <showSummary>false</showSummary>
      <anytime>true</anytime>
    
      <contentList>
      
        <prompt>
          <id>hoursOfSleep</id>
          <displayType>count</displayType>
          <displayLabel>Hours of Sleep</displayLabel>
          <promptText>How many hours in total did you sleep last night?</promptText>
          <promptType>number</promptType>
          <properties>
            <property>
              <key>min</key>
              <label>0</label>
            </property>
            <property>
              <key>max</key>
              <label>24</label>
            </property>
          </properties>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
        </prompt>

        <prompt>
          <id>mood1</id>
          <displayType>category</displayType>
          <displayLabel>How would you describe your mood at this moment</displayLabel>
          <promptText>How would you describe your mood at this moment?</promptText>
          <promptType>multi_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Sad</label>
            </property>
            <property>
              <key>1</key>
              <label>Calm/Relaxed</label>
            </property>
            <property>
              <key>2</key>
              <label>Stressed/Anxious</label>
            </property>
            <property>
              <key>3</key>
              <label>Tired</label>
            </property>
            <property>
              <key>4</key>
              <label>Happy</label>
            </property>
            <property>
              <key>5</key>
              <label>Upset</label>
            </property>
            <property>
              <key>6</key>
              <label>Energetic</label>
            </property>
            <property>
              <key>7</key>
              <label>Irritable</label>
            </property>
            <property>
              <key>8</key>
              <label>Bored</label>
            </property>
            <property>
              <key>9</key>
              <label>Focused</label>
            </property>
          </properties>
        </prompt>

        <prompt>
          <id>feltStress1</id>
          <displayType>measurement</displayType>
		  <unit>how stressed</unit>
          <displayLabel>Felt Stress</displayLabel>
          <promptText>Have you felt stressed in the last two hours? </promptText>
          <promptType>single_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Not at all</label>
              <value>1</value>
            </property>
            <property>
              <key>1</key>
              <label>Slightly</label>
              <value>2</value>
            </property>
            <property>
              <key>2</key>
              <label>Moderately</label>
              <value>3</value>
            </property>
            <property>
              <key>3</key>
              <label>Very</label>
              <value>4</value>
            </property>
           </properties>
        </prompt>

        <prompt>
          <id>whatEvent1</id>
          <condition>feltStress1 > 0</condition>
          <displayType>category</displayType>
          <displayLabel>Cause</displayLabel>
          <promptText>What was the cause of your stress?</promptText>
          <promptType>multi_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Finances</label>
            </property>
            <property>
              <key>1</key>
              <label>Work/School</label>
            </property>
            <property>
              <key>2</key>
              <label>Lack of control</label>
            </property>
            <property>
              <key>3</key>
              <label>Family/Relationships</label>
            </property>
            <property>
              <key>4</key>
              <label>Health</label>
            </property>
            <property>
              <key>5</key>
              <label>Traffic</label>
            </property>
            <property>
              <key>6</key>
              <label>Other</label>
            </property>
          </properties>
        </prompt>
    
        <prompt>
          <id>eatenRecently1</id>
          <displayType>category</displayType>
          <displayLabel>Eaten since your last survey</displayLabel>
          <promptText>Have you eaten since you completed your last diet survey?</promptText>
          <promptType>single_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>No</label>
            </property>
            <property>
              <key>1</key>
              <label>Yes</label>
            </property>
          </properties>
        </prompt>

        <prompt>
          <id>foodWhoAteWith1</id>
          <condition>eatenRecently1 == 1</condition>
          <displayType>category</displayType>
          <displayLabel>WhoWith</displayLabel>
          <promptText>Who did you eat with?</promptText>
          <promptType>multi_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Alone</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Friends</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Family</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Co-workers</label>
              <value>3</value>
            </property>
            <property>
              <key>4</key>
              <label>Other</label>
              <value>4</value>
            </property>
          </properties>
        </prompt>

      
        <prompt>
           <id>foodSnackMeal1</id>
          <condition>eatenRecently1 == 1</condition>
           <displayType>category</displayType>
           <displayLabel>Type</displayLabel>
           <promptText>Was this a snack or a meal?</promptText>
           <promptType>single_choice</promptType>

          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Snack</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Meal</label>
              <value>1</value>
            </property>
          </properties>
        </prompt>

        <prompt>
          <id>foodHowPrepared1</id>
          <condition>eatenRecently1 == 1</condition>
          <displayType>category</displayType>
          <displayLabel>HowPrepared</displayLabel>
          <promptText>How was this food prepared?</promptText>
          <promptType>multi_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Restaurant</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Fast food</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Home made</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Prepared (i.e., deli, salad bar) </label>
              <value>3</value>
            </property>
            <property>
              <key>4</key>
              <label>Frozen dinner</label>
              <value>4</value>
            </property>
            <property>
              <key>5</key>
              <label>Other </label>
              <value>5</value>
            </property>
          </properties>
        </prompt>

        <prompt>
          <id>foodQuality1</id>
          <condition>eatenRecently1 == 1</condition>
          <displayType>category</displayType>
          <displayLabel>NutritionalQuality</displayLabel>
          <promptText>Rate the nutritional quality of this meal?</promptText>
          <promptType>single_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Low</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Medium</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>High</label>
              <value>2</value>
            </property>
          </properties>
        </prompt>
        
        
        <prompt>
          <id>foodHowHungry1</id>
          <condition>eatenRecently1 == 1</condition>
          <displayType>count</displayType>
          <displayLabel>Hunger</displayLabel>
          <unit>hunger amount</unit>
          <promptText>How hungry were you before eating?</promptText>
          <promptType>single_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Very</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Moderately</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Slightly</label>
              <value>2</value>
            </property>
            <property>
              <key>3</key>
              <label>Not at all</label>
              <value>3</value>
            </property>
          </properties>
        </prompt>


        <prompt>
          <id>foodHowMuch1</id>
          <condition>eatenRecently1 == 1</condition>
          <displayType>category</displayType>
          <displayLabel>HowMuch</displayLabel>
          <promptText>How much did you eat?</promptText>
          <promptType>single_choice</promptType>
          <skippable>true</skippable>
          <skipLabel>skip</skipLabel>
          <properties>
            <property>
              <key>0</key>
              <label>Too little / not enough</label>
              <value>0</value>
            </property>
            <property>
              <key>1</key>
              <label>Just right</label>
              <value>1</value>
            </property>
            <property>
              <key>2</key>
              <label>Too much</label>
              <value>2</value>
            </property>
          </properties>
        </prompt>

      </contentList>
    </survey>

