<campaign>
	<campaignUrn>urn:campaign:formatting</campaignUrn>
	<campaignName>Formatting Campaign</campaignName>
	<surveys>
		<survey>
			<id>timestampSurvey</id>
			<title>Timestamp Survey</title>
			<description>This is a survey to test behavior of the timestamp prompt type.</description>
			<submitText>Done with the timestamp survey.</submitText>
			<showSummary>false</showSummary>
			<editSummary>false</editSummary>
			<summaryText>Timestamp Test</summaryText>
			<anytime>true</anytime>
			<contentList>
				<prompt>
					<id>timestamp</id>
					<displayType>event</displayType>
					<displayLabel>mTimestamp</displayLabel>
					<promptText>What time is it?</promptText>
					<promptTextMarkdown>What time is it? ![Ohmage Logo](https://dev.ohmage.org/web/images/theme-ohmage/dashboard-header-logo.png)</promptTextMarkdown>
					<abbreviatedText>What time is it?</abbreviatedText>
					<promptType>timestamp</promptType>
					<skippable>true</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
			</contentList>
		</survey>
		<survey>
			<id>hoursBeforeNowSurvey</id>
			<title>Hours Before Now Survey</title>
			<description>This is a survey to test behavior of the hours_before_now prompt type.</description>
			<submitText>Done with the hours_before_now survey</submitText>
			<showSummary>false</showSummary>
			<editSummary>false</editSummary>
			<summaryText>Hours Before Now Test</summaryText>
			<anytime>true</anytime>
			<contentList>
				<prompt>
					<id>hoursBeforeNowNegTenToZero</id>
					<displayType>event</displayType>
					<displayLabel>hoursBeforeNowNegTenToZero</displayLabel>
					<promptText>Pick a number. Yay a number.</promptText>
					<promptTextMarkdown>Pick a number. ![Google Logo](https://sites.google.com/site/studentchromebook/event-resources-and-calendar/event-planning-print-materials/google_logo_shaded.jpg) Yay a number.</promptTextMarkdown>
					<abbreviatedText>Pick a number.</abbreviatedText>
					<promptType>hours_before_now</promptType>
					<properties>
						<property>
							<key>min</key>
							<label>-10</label>
						</property>
						<property>
							<key>max</key>
							<label>0</label>
						</property>
					</properties>
					<default>-10</default>
					<skippable>true</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
			</contentList>
		</survey>
		<survey>
			<id>numberSurvey</id>
			<title>Number Survey</title>
			<description>This is a survey to test behavior of the number prompt type.</description>
			<submitText>Done with the number survey</submitText>
			<showSummary>false</showSummary>
			<editSummary>false</editSummary>
			<summaryText>Number Test</summaryText>
			<anytime>true</anytime>
			<contentList>
				<prompt>
					<id>numberNegTenToZero</id>
					<displayType>event</displayType>
					<displayLabel>numberNegTenToZero</displayLabel>
					<promptText>Pick a number.</promptText>
					<promptTextMarkdown>![Facebook Logo](https://www.facebookbrand.com/img/assets/asset.facebook.logo.lg.png) Pick a number.</promptTextMarkdown>
					<abbreviatedText>Pick a number.</abbreviatedText>
					<promptType>number</promptType>
					<properties>
						<property>
							<key>min</key>
							<label>-10</label>
						</property>
						<property>
							<key>max</key>
							<label>0</label>
						</property>
					</properties>
					<default>-10</default>
					<skippable>true</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
			</contentList>
		</survey>
		<survey>
			<id>singleChoiceSurvey</id>
			<title>Single Choice Survey</title>
			<description>This is a survey to test behavior of the single_choice prompt type.</description>
			<submitText>Done with the single_choice survey</submitText>
			<showSummary>false</showSummary>
			<editSummary>false</editSummary>
			<summaryText>Single Choice Test</summaryText>
			<anytime>true</anytime>
			<contentList>
				<prompt>
					<id>singleChoice</id>
					<displayType>event</displayType>
					<displayLabel>Rivers</displayLabel>
					<promptText>Pick a Californian river from this list of main (non-tributary) rivers (listed from coastal North to South then inland North to South).</promptText>
					<abbreviatedText>California River</abbreviatedText>
					<promptType>single_choice</promptType>
					<properties>
						<property>
							<key>0</key>
							<label>Smith River ![item](http://upload.wikimedia.org/wikipedia/commons/thumb/6/62/DonauknieVisegrad.jpg/250px-DonauknieVisegrad.jpg)</label>
						</property>
						<property>
							<key>1</key>
							<label>Klamath River ![item](http://upload.wikimedia.org/wikipedia/commons/thumb/c/cf/Melting_Toe_of_Athabasca_Glacier.jpg/220px-Melting_Toe_of_Athabasca_Glacier.jpg)</label>
						</property>
						<property>
							<key>2</key>
							<label>Mad River ![item](http://upload.wikimedia.org/wikipedia/commons/thumb/5/5c/Loboc_river.png/220px-Loboc_river.png)</label>
						</property>
						<property>
							<key>3</key>
							<label>Eel River ![item](http://upload.wikimedia.org/wikipedia/commons/thumb/9/9e/The_River_Cam_from_the_Green_Dragon_Bridge.jpg/800px-The_River_Cam_from_the_Green_Dragon_Bridge.jpg)</label>
						</property>
						<property>
							<key>4</key>
							<label>Bear River ![item](http://upload.wikimedia.org/wikipedia/commons/thumb/e/ee/Braine-le-Ch%C3%A2teau_JPG02.jpg/220px-Braine-le-Ch%C3%A2teau_JPG02.jpg)</label>
						</property>
					</properties>
					<skippable>false</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
			</contentList>
		</survey>
		<survey>
			<id>singleChoiceSurveyNewLine</id>
